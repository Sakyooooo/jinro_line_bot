generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Room {
  id        String   @id @default(cuid())
  roomId    String   @unique
  passcode  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
